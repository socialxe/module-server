<query id="getClient" action="select">
	<tables>
		<table name="socialxeserver_member" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="client_token" var="client_token" />
		<condition operation="equal" column="client_srl" var="client_srl" />
	</conditions>
</query>
